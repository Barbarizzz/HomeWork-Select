public static void Array_arrays()

   {
       int arrray1 [] [] =new int[4][5];
       int i,j,k=0;
       for(i=0;i<4;i++)
           for(j=0;j<5;j++)
       {
           arrray1[i][j]=k;
           k++;
       }
       for (i=0;i<4;i++) {
           for (j = 0; j < 5; j++)
               System.out.print(arrray1[i][j] + " ");
               System.out.println();
           }
       }
